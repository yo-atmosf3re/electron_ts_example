{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "main": "public/main.ts",
  "browser": {
    "start": false,
    "fs": false,
    "os": false,
    "path": false
  },
  "scripts": {
    "electron": "wait-on tcp:8888 && electron .",
    "e:start": "concurrently -k \"cross-env BROWSER=none yarn w:start\" \"yarn run electron\"",
    "ef:start": "electron-forge start",
    "ef:package": "electron-forge package",
    "ef:make": "electron-forge make",
    "w:start": "webpack serve --env port=8888",
    "w:build": "webpack",
    "w:build:prod": "webpack --env mode=production",
    "w:build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts, tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts, tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@electron-forge/cli": "^7.1.0",
    "@electron-forge/maker-deb": "^7.1.0",
    "@electron-forge/maker-rpm": "^7.1.0",
    "@electron-forge/maker-squirrel": "^7.1.0",
    "@electron-forge/maker-zip": "^7.1.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.1.0",
    "@electron/remote": "^2.0.12",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@svgr/webpack": "6.2.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "6.6.0",
    "electron": "^24.3.0",
    "electron-is-dev": "^2.0.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "2.5.3",
    "react-app-rewired": "^2.2.1",
    "reg-cli": "^0.17.6",
    "regenerator-runtime": "^0.13.9",
    "sass": "1.49.9",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "wait-on": "^7.0.1",
    "webpack": "5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.7.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.26.1",
    "electron-squirrel-startup": "^1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1"
  }
}